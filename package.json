{
  "name": "superrare-landmarks",
  "version": "0.0.1",
  "description": "API for landmark lookup services",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "./scripts/dev.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "./scripts/test-e2e.sh",
    "ci": "./scripts/run-ci.sh",
    "pre-commit": "pnpm lint && pnpm test && pnpm test:e2e",
    "docker:logs": "./scripts/docker-logs.sh",
    "docker:logs:worker1": "./scripts/docker-worker-logs.sh",
    "docker:rerun": "docker compose down -v && docker compose up -d",
    "docker:rebuild": "docker compose down -v && docker compose up -d --build",
    "docker:redis-cli": "./scripts/redis-cli.sh",
    "sqlite:show-db": "./scripts/show-db.sh",
    "sqlite:show-db:watch": "./scripts/show-db.sh --watch"
  },
  "dependencies": {
    "@keyv/redis": "^4.3.1",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.4.1",
    "bullmq": "^5.41.7",
    "cache-manager": "^6.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.5.0",
    "keyv": "^5.3.1",
    "nestjs-zod": "^4.3.1",
    "ngeohash": "^0.6.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/keyv": "^4.2.0",
    "@types/ngeohash": "^0.6.8",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.2.5",
    "prisma": "^6.4.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.26.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "roots": [
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}